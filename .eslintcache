<TodoItem key={item.key} item={item} handleClick={handleClick} />\n      ))}\n      <div className=\"panel-block\">\n        {items.filter(i => key == 1 || key == 2 && i.done == false || key == 3 && i.done == true).length} items\n      </div>\n      <div className='panel-header'>\n        <button className='btn' onClick={clearItems}>全てのTodo保存</button>\n      </div>\n\n    </div>\n  );\n}\n\nexport default Todo;","/workspace/itss_std_react_todo_k63/src/components/TodoItem.js",[],"/workspace/itss_std_react_todo_k63/src/components/Filter.js",["63","64","65","66","67","68"],"/* \n  【Filterコンポーネント】\n　・該当するTodoをステータス毎にで分けてリスト表示する\n　・タブで表示する\n　・サポートするステータスは「すべて」「未完了」「完了済み」\n*/\nfunction Filter( {handleFilter,key} ) {\n  return (\n    <div className=\"panel-tabs\">\n      <a onClick={()=>handleFilter(1)} className={`${key == 1 ? 'is-active' : ''}`}>全て</a>\n      <a onClick={()=>handleFilter(2)} className={`${key == 2 ? 'is-active' : ''}`}>未完了</a>\n      <a onClick={()=>handleFilter(3)} className={`${key == 3 ? 'is-active' : ''}`}>完了済み</a>\n    </div>\n  );\n}\n\nexport default Filter","/workspace/itss_std_react_todo_k63/src/components/Input.js",["69"],"import React, { useState } from 'react';\n/* \n  【inputコンポーネント】\n　・新しいTodoを作成するINPUTフィールドを作成するコンポーネント\n　・Enterをクリックされたら入力された文字を使って新しいTodoを作成する\n*/\nfunction Input( {handleEnter} ) {\n  const [value,setValue] = useState('')\n  const handle = (e)=>{\n    if(e.key == \"Enter\"){\n      handleEnter(value)\n      setValue('')\n    }\n  }\n  return (\n    <div className=\"panel-block\">\n      <input type=\"text\" className='input' value={value} onKeyDown={handle} onChange={(e)=>{setValue(e.target.value)}}/>\n    </div>\n  );\n}\n\nexport default Input;\n","/workspace/itss_std_react_todo_k63/src/hooks/storage.js",[],"/workspace/itss_std_react_todo_k63/src/lib/util.js",[],{"ruleId":"70","replacedBy":"71"},{"ruleId":"72","replacedBy":"73"},{"ruleId":"74","severity":1,"message":"75","line":27,"column":17,"nodeType":"76","messageId":"7